{
  "name": "react-motion-menu",
  "version": "0.1.0",
  "main": "lib/index.js",
  "scripts": {
    "watch:example": "watchify --extension=js -o example/dist/bundle.js example/src/index.js",
    "compile": "babel --presets react -d lib/ src/",
    "build:example": "browserify --extension=js -o example/dist/bundle.js example/src/index.js",
    "prepublish": "npm run compile",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bokuweb/react-motion-menu.git"
  },
  "keywords": [
    "react",
    "menu",
    "motion",
    "component",
    "animation"
  ],
  "author": "bokuweb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bokuweb/react-motion-menu/issues"
  },
  "homepage": "https://github.com/bokuweb/react-motion-men",
  "peerDependencies": {
    "react": ">=0.14.3",
    "react-dom": ">=0.14.3",
    "react-motion": "^0.3.1"
  },
  "devDependencies": {
    "babelify": "6.4.0",
    "browserify": "^12.0.1",
    "watchify": "3.5.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "files": [
    "lib"
  ],
  "directories": {
    "example": "example"
